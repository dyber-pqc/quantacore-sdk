ngx_addon_name=ngx_http_quac_module

if test -n "$ngx_module_link"; then
    ngx_module_type=HTTP
    ngx_module_name=ngx_http_quac_module
    ngx_module_srcs="$ngx_addon_dir/ngx_http_quac_module.c"
    ngx_module_libs="-L/usr/local/lib -lquac_tls -lssl -lcrypto -lpthread"
    ngx_module_incs="/usr/local/include"
    
    . auto/module
else
    HTTP_MODULES="$HTTP_MODULES ngx_http_quac_module"
    NGX_ADDON_SRCS="$NGX_ADDON_SRCS $ngx_addon_dir/ngx_http_quac_module.c"
    CORE_LIBS="$CORE_LIBS -L/usr/local/lib -lquac_tls -lssl -lcrypto -lpthread"
    CORE_INCS="$CORE_INCS /usr/local/include"
fi
